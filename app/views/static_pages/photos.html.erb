
<div class="large"  style="width:100%;">
  <div class="row">
      <% @photos.each do |photo| %>
        <div class="image">
          <a href="<%= photo.medium500.source_url %>">
          <%= image_tag photo.square150.source_url %>
        </div> 
      <% end %>
  </div>
  <%= will_paginate @photos %>
</div>

